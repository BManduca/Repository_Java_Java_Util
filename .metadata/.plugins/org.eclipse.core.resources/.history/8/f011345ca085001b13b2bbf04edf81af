
public class Teste {
	
	//array -> []
	public static void main(String[] args) {
		
		/*
		 * Arrays
		 * -> int idades[] ou int[] idades
		 * */
		
		//o array inicializa cada posicao com 0
		int idades[] = new int[5];
		
		int idade1 = idades[0];
		
		System.out.println("Idade1 = " + idade1);
		
		/*int idade1 = 29;
